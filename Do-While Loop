class HelloWorld {
    public static void main(String[] args) {
        System.out.println(1);
        System.out.println(2);
        System.out.println(3);
        System.out.println("Now by using loop");
        int i = 1;
        do{
            System.out.println(i);
            
        i++;
        }while(i<=5);
    }
}
